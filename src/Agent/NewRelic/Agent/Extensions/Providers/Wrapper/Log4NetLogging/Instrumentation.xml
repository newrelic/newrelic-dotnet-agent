<extension xmlns="urn:newrelic-extension">
  <instrumentation>
    <tracerFactory name="log4net">
      <match assemblyName="log4net" className="log4net.Repository.Hierarchy.Logger">
        <exactMethodMatcher methodName="CallAppenders" />
      </match>
    </tracerFactory>
  </instrumentation>
</extension>
